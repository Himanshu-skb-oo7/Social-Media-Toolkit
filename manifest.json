{
  "manifest_version":2,
  "name": "Social Media ToolKit ",
  "description": "Allow you to share text, images, links, pages etc to twitter",
  "icons":{
    "64": "icon.png",
    "32": "icon.png",
    "16": "icon.png",
    "128": "icon.png"
  },
  "background":{
    "scripts":["background.js"]
  },
  "version": "1.0",

  "permissions": [
    "contextMenus","tabs","browsingData"
  ]

}